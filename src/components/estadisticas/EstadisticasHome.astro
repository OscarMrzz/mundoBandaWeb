---
import GraficaBarraPremier from "./graficas/GraficaBarrasPremier";
import GraficaLiniasPremier from "./graficas/GraficaLiniasPremier";
---

<div class="pt-15">
  <section class="mb-10">
    <h1 class="text-4xl text-white font-black px-10">ESTADISTICAS</h1>
    <h2 class="text-2xl font-bold text-gray-200 px-10">LINIA DE TIEMPO</h2>
    <h3 class="text-2xl text-gray-300 px-10">PREMIER</h3>
    <GraficaLiniasPremier client:load />
  </section>
  <section>
    <h2 class="text-2xl font-bold text-gray-200 px-10">TOTAL DE PUNTOS</h2>
    <h3 class="text-2xl text-gray-300 px-10">PREMIER</h3>

    <GraficaBarraPremier client:visible />
  </section>
</div>
