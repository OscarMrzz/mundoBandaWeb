
---
import CampeonesRecientesPorEvento from "../components/campeonesRecientesPorEvento.astro"
import Layout from "../layouts/Layout.astro"
import LayoutPaginasInternas from "../layouts/LayoutPaginasInternas.astro"
import campeonesRecintes from "../lib/data/campeonesRecintes.json"
const categoriasFECCHA =["PREMIER", "A","B"]
---

<Layout title="Campeones Recientes - Mundo Banda" description="Descubre los campeones recientes en el mundo de las bandas. Mantente al día con los últimos ganadores y eventos destacados." >
<div class="  ">
    <div class="flex justify-center  pt-20 pb-10 pl-10">

        <h1 class="text-4xl font-bold text-white">CAMPEONES <span class="text-orange-500">RECIENTES</span> </h1>
    </div>
<div>
    <CampeonesRecientesPorEvento
        evento="Comp. Leon ALvarado"
        Federacion="FCCAH"
        campeones={campeonesRecintes}
        categorias={categoriasFECCHA}
    />

</div>

 
</div>
</Layout>
